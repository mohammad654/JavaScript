


                                    Json JavaScript Object Notation

                       JSON.parse        : text to json
                       JSON.stringify    : json to text



    var url="https://randomuser.me/api/?results=5#";
        fetch(url).then(response=>{return response.json()}).then(data=>{
            console.log(data);
        }).catch(error=>{
            console.log(error);
        })
